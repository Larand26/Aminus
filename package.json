{
  "name": "aminus",
  "private": true,
  "version": "2.24.0",
  "main": "main.js",
  "description": "Um sistema capaz de alterar e ver um banco de dados",
  "repository": {
    "type": "git",
    "url": "https://github.com/Larand26/Aminus"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "release": "npm run build && npm run make",
    "publish-app": "npm run build && dotenv -- electron-forge publish",
    "dev:all": "npm-run-all --parallel dev start"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@google/genai": "^1.32.0",
    "axios": "^1.11.0",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.5.0",
    "electron-squirrel-startup": "^1.0.1",
    "env": "^0.0.2",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "jszip": "^3.10.1",
    "mongoose": "^8.15.1",
    "mssql": "^11.0.1",
    "mysql2": "^3.14.1",
    "ospath": "^1.2.2",
    "path": "^0.12.7",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^3.6.0",
    "update-electron-app": "^3.1.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/publisher-github": "^7.11.1",
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "dotenv-cli": "^11.0.0",
    "electron": "^36.3.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "vite": "^6.3.5"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./build/icon",
        "extraFiles": [
          "./db",
          "./config",
          "./gemini",
          "./token",
          "./transportadoras",
          "./erros"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "aminus",
            "setupIcon": "./build/icon.ico",
            "authors": "Victor de Brito Laranjeira"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "Larand26",
              "name": "Aminus"
            },
            "prerelease": false
          }
        }
      ]
    }
  }
}
